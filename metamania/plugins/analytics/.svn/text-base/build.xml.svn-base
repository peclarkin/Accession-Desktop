<?xml version="1.0" encoding="UTF-8"?>
<!-- Created by Emil Ivov -->

<project default="make" name="analytics">

    <!-- import the abstract plugin build.xml -->
    <import file="../abstract-plugin-build.xml"/>

    <!--BUNDLES -->
    <target name="plugin-bundles" depends="bundle-analytics"/>

    <!-- bundle-emptyplugin -->
    <target name="bundle-analytics">
        <jar compress="false"
             destfile="${release}/analytics.jar"
             manifest="src/net/java/sip/communicator/impl/analytics/analytics.manifest.mf">
            <zipfileset dir="classes/net/java/sip/communicator/impl/analytics"
                        prefix="net/java/sip/communicator/impl/analytics"/>
            <zipfileset dir="classes/net/java/sip/communicator/service/analytics"
                        prefix="net/java/sip/communicator/service/analytics" />
        </jar>
    </target>
</project>
